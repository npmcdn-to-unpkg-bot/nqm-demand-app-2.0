        script(src='../L.GeoSearch/js/l.control.geosearch.js')
        script(src='../L.GeoSearch/js/l.geosearch.provider.openstreetmap.js')
        link(rel='stylesheet', href='../L.GeoSearch/css/l.geosearch.css')
  
        

